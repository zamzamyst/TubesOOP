<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="ReportController">
    <children>
        <!-- Label untuk Judul -->
        <Label layoutX="219.0" layoutY="14.0" text="Laporan Harian">
         <font>
            <Font size="24.0" />
         </font></Label>
        
        <!-- TextArea untuk Menampilkan Laporan -->
        <TextArea fx:id="reportTextArea" editable="false" layoutX="50" layoutY="60" prefHeight="280" prefWidth="500" wrapText="true" />

        <!-- Button untuk Kembali -->
        <Button fx:id="btnBack" layoutX="250" layoutY="360" prefWidth="100" text="Kembali" />
    </children>
</AnchorPane>
